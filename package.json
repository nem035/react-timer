{
  "name": "react-simple-timer",
  "version": "1.0.2",
  "description": "Simple timer application, writen with React & Foundation",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "deploy": "git push heroku master",
    "dev": "webpack -w",
    "git": "npm run build && git add --all && git commit -am \"$(date)\"",
    "lint": "eslint --ext .jsx --ext .js ./app/*;",
    "major": "npm run git && npm version major && npm run push",
    "minor": "npm run git && npm version minor && npm run push",
    "patch": "npm run git && npm version patch && npm run push",
    "push": "git push origin master --tags",
    "start": "node server.js",
    "test": "karma start"
  },
  "author": "Nemanja Stojanovic",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.13.1",
    "express": "^4.14.0",
    "foundation-sites": "^6.2.3",
    "jquery": "^3.0.0",
    "react": "^15.3.0",
    "react-addons-test-utils": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-router": "^2.6.1"
  },
  "devDependencies": {
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "css-loader": "^0.23.1",
    "eslint": "^3.3.0",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-plugin-import": "^1.13.0",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-react": "^6.0.0",
    "expect": "^1.20.2",
    "karma": "^1.2.0",
    "karma-chrome-launcher": "^1.0.1",
    "karma-mocha": "^1.1.1",
    "karma-mocha-reporter": "^2.1.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "mocha": "^3.0.2",
    "node-sass": "^3.8.0",
    "sass-loader": "^4.0.0",
    "script-loader": "^0.7.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1"
  }
}
